{
    "name": "zhuk/laravel-intercom",
    "description": "Intercom client and notifications in Laravel",
    "type": "library",
    "homepage": "https://github.com/victorzhuk/laravel-intercom",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zhuk\\LaravelIntercom\\": "src"
        }
    },
    "authors": [
        {
            "name": "Victor Zhuk",
            "email": "zhuk@pm.me"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zhuk\\LaravelIntercom\\IntercomServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=7.4",
        "intercom/intercom-php": "^4.4",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2",
        "phpro/grumphp": "^1.5",
        "vimeo/psalm": "^4.10"
    }
}
